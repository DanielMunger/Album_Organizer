<h1>Search Results</h1>
<ul>
  @if(Model.Count == 0)
  {
    <h3>There were no Artists that matched your search.</h3>
  }
  @foreach(var artist in Model)
  {
    @foreach(var album in artist.GetAlbum())
    {
      <li>@album.GetTitle() by: @artist.GetName()</li>
    }
  }
</ul>
<p>
  <a href="/">See your collection</a><br>
  <a href="/album/new">Add another album</a><br>
  <a href="/artist_search_form">Search Your Collection</a>
</p>
